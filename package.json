{
  "name": "@buggyorg/lisgy",
  "version": "0.2.0-pre.3",
  "description": "",
  "main": "lib/lisgy.js",
  "bin": {
    "lisgy": "./lib/cli.js",
    "lisgy-check": "./lib/valid.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "pretest": "npm run build",
    "test": "dot-only-hunter && nyc mocha --compilers js:babel-register --timeout 15000 --recursive && standard src/**/*.js",
    "test:dev": "nyc mocha --compilers js:babel-register --timeout 15000 --recursive && standard src/**/*.js",
    "test:watch": "mocha --compilers js:babel-register --timeout 15000 --recursive --watch",
    "prepublish": "babel src -d lib",
    "report": "nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "**/src/**/*.js",
      "**/test/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@buggyorg/component-library": "0.3.7",
    "@buggyorg/graphlib2kgraph": "0.1.11",
    "@buggyorg/graphtools": "0.4.0-pre.27",
    "@buggyorg/resolve": "0.2.9",
    "chalk": "^1.1.3",
    "cli-ext": "^1.0.0",
    "commander": "^2.9.0",
    "get-stdin": "^5.0.1",
    "graphlib": "^2.1.0",
    "jsedn": "BuggyOrg/jsedn#b14ba69ef0b3e95e91b43b94f6a57350610fd1f5",
    "lodash": "^4.8.2",
    "tempfile": "^1.1.1",
    "wsl-lisp-parser": "^0.1.0",
    "yargs": "^7.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.0",
    "chai-as-promised": "^6.0.0",
    "chai-subset": "^1.3.0",
    "coveralls": "^2.12.0",
    "dot-only-hunter": "^1.0.3",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "standard": "^9.0.1"
  }
}
